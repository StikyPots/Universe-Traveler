{
  "name": "GameAdventure",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
        "$path": "src/ReplicatedStorage",

        "Libraries": {
          "$path": "Packages"
        },

        "$ignoreUnknownInstances": true
    },

    "ServerScriptService": {
      "$path": "src/ServerScriptService",
      "$ignoreUnknownInstances": true
    },

    "ServerStorage": {
      "$path": "src/ServerStorage",

      "Libraries": {
        "$path": "ServerPackages"
      },
      "$ignoreUnknownInstances": true
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
          "$path": "src/StarterPlayerScripts"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      
      "PlaceholderTowers": {
        "$className": "Folder"
      },

      "Map":{
        "$className": "Folder",

        "Entities":{
          "$className": "Folder"
        },

        "Towers":{
          "$className": "Folder"
        }
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}